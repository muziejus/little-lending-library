<h1>{{ full_name }}</h1>
<div class="row">
  <div class="col-md-4 ">
    <h2>{{ tags }}</h2>
    <h4><a href="{{ url }}" target="_blank">{{ url }}</a></h4>
    <h3>Addresses:</h3>
    <ol>
      {{#address}}
        <li>
          <a class="btn btn-xs btn-primary" href="/addresses/{{ address_id }}">{{ address_id }}</a>
          <a href="#" onClick="recenter({{ latitude }}, {{ longitude }})">
            {{ street }}, {{ city }}, {{ state }}, {{ postal_code }}, {{ country }}
          </a>
        </li>
      {{/address}}
    </ol>
    <h3>Associations:</h3>
    <ol>
      {{#association}}
        <li>
          <a class="btn btn-xs btn-primary" href="/entities/{{ association_id }}">{{ association_id }}</a>
          {{ association_full_name }}
        </li>
      {{/association}}
    </ol>
  </div>
  {{> partials/map }}
</div>
